'''x = [[0,1,2],[3,4,5],[6,7,8]]
y = [[1,2,3],[4,5,6],[7,8,9]]
a = ((x[0][0])+y[0][0])
b = ((x[0][1])+y[0][1])
c = ((x[0][2])+y[0][2])
d = ((x[1][0])+y[1][0])
e = ((x[1][1])+y[1][1])
f = ((x[1][2])+y[1][2])
g = ((x[2][0])+y[2][0])
