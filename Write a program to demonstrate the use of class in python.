class RailwayForm:
    def info(self="Abhishek"):
        name = input("Enter your name: ")

    def PRINT(self="Abhsihek"):
        print("Your registration will successfully done")


Abhi = RailwayForm
Vedant = RailwayForm
Abhi.info()
Vedant.info()
Abhi.PRINT()
Vedant.PRINT()
